{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "",
    "outDir": "",
    "importHelpers": false,
    "declaration": true
  },
  "exclude": [
    "node_modules"
  ]
}